{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "/*.html", "use": "@vercel/static" },
    { "src": "/*.js", "use": "@vercel/static" },
    { "src": "/*.css", "use": "@vercel/static" }
  ],
  "cleanUrls": true
}
